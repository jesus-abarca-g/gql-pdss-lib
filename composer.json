{
    "name": "codewebapp/gqlpdss",
    "description": "Utilidades para crear una api GraphQL",
    "type": "library",
    "require": {
        "php": "^7.1 || ^8.0",
        "doctrine/orm": "^2.10.2",
        "doctrine/dbal": "^3.1.1",
        "symfony/yaml": "2.*",
        "symfony/cache": "^5.3",
        "webonyx/graphql-php": "^14.11",
        "laminas/laminas-servicemanager": "^3.7",
        "ecodev/graphql-doctrine": "^7.1",
        "nikic/fast-route": "^1.3",
        "flowjs/flow-php-server": "^1.0",
        "phpoffice/phpspreadsheet": "^1.19"
    },
    "autoload": {
        "files": [
            "gqlpdss/GPDCore/src/Functions/PaginationUtilities.php",
            "gqlpdss/GPDCore/src/Functions/StringFunctions.php",
            "gqlpdss/GPDCore/src/Functions/FileFunctions.php"
        ],
        "psr-4": {
            "GPDCore\\": "gqlpdss/GPDCore/src/"
        }
    }
}
